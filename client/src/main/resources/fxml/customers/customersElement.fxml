    <children>
        <Label fx:id="lblPrename" text="&lt;Prename&gt;" HBox.hgrow="ALWAYS">
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </Label>
        <Label fx:id="lblLastname" text="&lt;Lastname&gt;" HBox.hgrow="ALWAYS">
            <HBox.margin>
                <Insets />
            </HBox.margin>
        </Label>
        <Label fx:id="lbEMail" layoutX="89.0" layoutY="20.0" text="&lt;EMail&gt;" HBox.hgrow="ALWAYS" />
