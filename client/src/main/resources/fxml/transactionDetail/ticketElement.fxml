<?import javafx.scene.layout.HBox?>

<HBox prefHeight="100.0" prefWidth="200.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.112"
    xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="at.ac.tuwien.inso.sepm.ticketline.client.gui.transactions.TicketElementController">
    <children>
        <Label fx:id="lbPlace" text="&lt;Place&gt;">
            <HBox.margin>
                <Insets/>
            </HBox.margin>
        </Label>
        <Label fx:id="lbPrice" text="&lt;Price&gt;"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</HBox>
