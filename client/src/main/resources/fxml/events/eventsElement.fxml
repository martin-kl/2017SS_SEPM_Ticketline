<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<VBox xmlns="http://javafx.com/javafx/8.0.92"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="at.ac.tuwien.inso.sepm.ticketline.client.gui.news.NewsElementController"
      spacing="5.0">
    <stylesheets>
        <URL value="@/css/newsElement.css"/>
    </stylesheets>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label fx:id="lblDate" styleClass="newsPublishDate" text="&lt;DD.MM.YYYY | hh:mm&gt;"/>
    <Label fx:id="lblTitle" styleClass="newsTitle" text="&lt;NewsTitle&gt;"/>
    <Label fx:id="lblText" styleClass="newsText" wrapText="true" text="&lt;NewsText&gt;"/>
</VBox>
