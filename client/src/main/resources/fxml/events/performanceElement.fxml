<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.tuwien.inso.sepm.ticketline.client.gui.events.PerformanceElementController">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="5.0">
         <children>
            <Label fx:id="labelPerformanceName" text="PerformanceName">
               <padding>
                  <Insets bottom="4.0" left="4.0" right="20.0" top="4.0" />
               </padding></Label>
            <Label fx:id="labelStartTime" text="StartTime" />
            <Label text="-" />
            <Label fx:id="labelEndTime" text="EndTime">
               <padding>
                  <Insets right="15.0" />
               </padding></Label>
            <Label fx:id="labelLocationLabelling" text="Location" />
            <Label fx:id="labelLocationName" text="LocationName">
               <padding>
                  <Insets right="15.0" />
               </padding></Label>
            <Label fx:id="labelTypeLabelling" text="Type" />
            <Label fx:id="labelType" text="LocationType" />
         </children>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
