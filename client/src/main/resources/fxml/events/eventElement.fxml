<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.net.URL?>
<VBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.tuwien.inso.sepm.ticketline.client.gui.events.EventElementController">
    <stylesheets>
        <URL value="@/css/newsElement.css" />
    </stylesheets>
   <VBox spacing="10.0">
      <children>
         <Label fx:id="labelEventName" alignment="BOTTOM_LEFT" text="EventName">
            <font>
               <Font name="System Bold" size="16.0" />
            </font>
         </Label>
         <Label fx:id="labelDescription" text="Description" textFill="#00000099" />
         <HBox spacing="5.0">
            <children>
               <Label fx:id="labelEventCategoryLabelling" text="Category" />
               <Label fx:id="labelEventCategory" text="EventCategory" />
            </children>
         </HBox>
         <GridPane>
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
             <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
               <Label fx:id="labelPerformancenameLabelling" text="Label">
                  <font>
                     <Font name="System Bold" size="12.0" />
                  </font>
               </Label>
               <Label fx:id="labelBegintimeLabelling" text="Label" GridPane.columnIndex="1">
                  <font>
                     <Font name="System Bold" size="12.0" />
                  </font>
               </Label>
               <Label fx:id="labelEndtimeLabelling" text="Label" GridPane.columnIndex="2">
                  <font>
                     <Font name="System Bold" size="12.0" />
                  </font>
               </Label>
               <Label fx:id="labelLocationLabelling" text="Label" GridPane.columnIndex="3">
                  <font>
                     <Font name="System Bold" size="12.0" />
                  </font>
               </Label>
               <Label fx:id="labelTypeLabelling" text="Label" GridPane.columnIndex="4">
                  <font>
                     <Font name="System Bold" size="12.0" />
                  </font>
               </Label>
            </children>
            <VBox.margin>
               <Insets />
            </VBox.margin>
            <padding>
               <Insets bottom="10.0" left="20.0" top="10.0" />
            </padding>
         </GridPane>
      </children>
   </VBox>
   <VBox fx:id="vbPerformanceElements">
      <padding>
         <Insets bottom="10.0" />
      </padding></VBox>
</VBox>
