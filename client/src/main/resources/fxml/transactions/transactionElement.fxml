<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vbReservationAndTickets" spacing="5.0" styleClass="customersElement" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.tuwien.inso.sepm.ticketline.client.gui.transactions.TransactionListElementController">
    <children>
        <HBox fx:id="hbReservation">
            <children>
                <Label fx:id="lbCustomerFirstName" text="&lt;CustomerFirstName&gt;">
                    <HBox.margin>
                        <Insets right="5.0" />
                    </HBox.margin>
                </Label>
                <Label fx:id="lbCustomerLastName" layoutX="10.0" layoutY="10.0" text="&lt;CustomerLastName&gt;">
                    <HBox.margin>
                        <Insets right="10.0" />
                    </HBox.margin>
                </Label>
                <Label fx:id="lbReservationID" text="&lt;ReservationID&gt;">
                    <HBox.margin>
                        <Insets right="10.0" />
                    </HBox.margin>
                </Label>
            <Label fx:id="lbPerformanceName" text="&lt;PerformanceName&gt;">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Label>
                <Label fx:id="lbBoughtReserved" text="&lt;Bought/Reserved&gt;" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
